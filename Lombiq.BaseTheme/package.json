{
  "private": true,
  "dependencies": {
    "bootstrap": "^5.2.3"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.11.6"
  },
  "scripts": {
    "build": "npm explore nodejs-extensions -- pnpm build",
    "clean": "npm explore nodejs-extensions -- pnpm clean",
    "watch": "npm explore nodejs-extensions -- pnpm watch"
  },
  "nodejsExtensions": {
    "assetsToCopy": [
      {
        "sources": [ "Assets" ],
        "pattern": "Theme.png",
        "target": "wwwroot"
      },
      {
        "sources": [ "node_modules/bootstrap/dist" ],
        "target": "wwwroot/vendors/bootstrap"
      }
    ]
  }
}
